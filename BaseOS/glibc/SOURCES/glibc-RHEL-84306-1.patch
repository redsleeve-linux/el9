+      v = NAN;								\
+      goto out;								\
+    }									\
+									\
+  ch = read_input ();							\
+  if (ch != 'X' && ch != 'x')						\
+    {									\
+      err = ch < 0 ? ch : INPUT_FORMAT;					\
+      v = NAN;								\
+      goto out;								\
+    }									\
+									\
