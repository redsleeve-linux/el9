+	      {								\
+		err = ch < 0 ? ch : INPUT_FORMAT;			\
+		v = NAN;						\
+		goto out;						\
+	      }								\
+	    ch = read_input ();						\
+	  }								\
+	if (ch == ':')							\
+	  {								\
+	    v = m ? -INFINITY : +INFINITY;				\
+	    goto out;							\
+	  }								\
+      }									\
+      err = ch < 0 ? ch : INPUT_FORMAT;					\
+      v = NAN;								\
+      goto out;								\
+									\
+    case 'N':								\
+    case 'n':								\
