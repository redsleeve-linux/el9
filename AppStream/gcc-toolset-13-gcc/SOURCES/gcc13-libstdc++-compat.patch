+  std::terminate_handler thandler;
+  std::unexpected_handler uhandler;
+
+  void terminate_handler_wrapper ()
+  {
+    std::terminate_handler h;
+    {
+      __gnu_cxx::__scoped_lock l(mx);
+      h = thandler;
+    }
+    h ();
+  }
+
+  void unexpected_handler_wrapper ()
+  {
+    std::terminate_handler h;
+    {
+      __gnu_cxx::__scoped_lock l(mx);
+      h = uhandler;
+    }
+    h ();
+  }
+}
