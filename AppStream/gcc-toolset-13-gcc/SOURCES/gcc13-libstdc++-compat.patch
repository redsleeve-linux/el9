+	  --mode=link $(CXX) \
+	  $(VTV_CXXLINKFLAGS) \
+	  $(OPT_LDFLAGS) $(SECTION_LDFLAGS) $(AM_CXXFLAGS) $(LTLDFLAGS) -o $@
