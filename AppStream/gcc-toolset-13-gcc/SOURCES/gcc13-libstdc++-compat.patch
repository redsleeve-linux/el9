+std::terminate_handler
+std::get_terminate () noexcept
+{
+  __gnu_cxx::__scoped_lock l(mx);
+  thandler = std::set_terminate (terminate_handler_wrapper);
+  std::set_terminate (thandler);
+  return thandler;
